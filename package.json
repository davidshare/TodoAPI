{
  "name": "todo-api",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=production yarn build && yarn run serve",
    "start:dev": "NODE_ENV=dev nodemon --exec babel-node src/server.js",
    "build": "babel src --out-dir ./dist --source-maps",
    "dev:migrate": "NODE_ENV=dev babel-node src/database/migrations/index.js",
    "migrate": "NODE_ENV=production babel-node src/database/migrations/index.js",
    "coveralls": "jest --coverage --forceExit --coverageReporters=text-lcov | coveralls",
    "test": "NODE_ENV=test jest --coverage --detectOpenHandles --forceExit",
    "serve": "NODE_ENV=production && node dist/server.js",
    "lint": "eslint '**/*.{js,jsx}' --quiet",
    "lint:fix": "eslint '**/*.{js,jsx}' --fix"
  },
  "repository": "https://github.com/davidshare/TodoAPI.git",
  "author": "davidshare <davidessienshare@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "bcrypt": "^3.0.6",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^7.11.0",
    "supertest": "^4.0.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "coveralls": "^3.0.3",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "husky": "^2.3.0",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1"
  }
}
